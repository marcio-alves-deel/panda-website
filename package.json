{
  "name": "pandaro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ap.cx/react-fullpage": "^0.1.10",
    "@fullpage/react-fullpage": "^0.1.17",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "^1.3.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/reach__router": "^1.3.0",
    "@types/react": "^16.9.0",
    "@types/react-div-100vh": "^0.3.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "fullpage-react": "^3.1.0",
    "hide-scrollbar-react": "^1.1.6",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-awesome-reveal": "^2.4.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-div-100vh": "^0.3.8",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.3",
    "react-image-lightbox": "^5.1.1",
    "react-moment": "^0.9.7",
    "react-page-scroller": "^2.0.7-beta",
    "react-reveal": "^1.2.2",
    "react-scripts": "3.4.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "use-query-params": "^0.6.0"
  },
  "devDependencies": {
    "husky": "4.2.2",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0",
    "prettier": "^1.19.1",
    "lint-staged": "10.0.7",
    "app-json-env-gen": "0.6.0"
  },
  "scripts": {
    "start": "PORT=80 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "eject": "react-scripts eject",
    "format": "npm run prettier --config .prettierrc --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint --config .eslintrc --fix \"src/**/*.{ts, tsx, js, jsx}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tsconfig.json --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
